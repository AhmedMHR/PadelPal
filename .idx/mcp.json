{
  "mcpServers": {
    "firebase": {
      "command": "npx",
      "args": [
        "-y",
        "firebase-tools@latest",
        "experimental:mcp"
      ]
    },
    "next": {
      "command": "npm",
      "args": [
        "run",
        "dev"
      ],
      "env": {
        "PORT": "3000"
      }
    }
  }
}